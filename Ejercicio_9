n = [(1,4),(4,8),("hola"),(23),("Si"),(32,9),("20"),(100)]

print (n[0:1])
print (n[2:4])
print (n[3:9])

print ("--------------------------------------------------------------")

if (23) in n:
    print ("si esta")
else:
    print ("no esta")

print ("--------------------------------------------------------------")

for i in n:
    print (i[0:1])
    print (n[2:4])
    print (n[3:9])

